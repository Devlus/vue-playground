{
  "name": "vue-playground",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Devon Herlitz <herlitzd@husky.neu.edu>",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run test && npm run compile",
    "build:parts": "npm run sass && npm run compile",
    "ci:jenkins": "karma --env=jk start karma.coverage.js && npm run coverage:remap",
    "clean": "rimraf dist && rimraf coverage",
    "compile": "webpack --config config/webpack.config.prod.js",
    "coverage": "npm run coverage:run && npm run coverage:remap && npm run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "dev": "webpack-dev-server --hot --inline",
    "lint": "tslint src/**/*.ts",
    "serve": "http-server dist/ -o",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch",
    "e2e": "./node_modules/.bin/cypress run",
    "e2e:serve": "concurrently -c \"cyan,magenta\" --names \"web,cy\" \"npm run serve\" \"npm run e2e\"",
    "e2e:debug": "./node_modules/.bin/cypress open",
    "e2e:debug:serve": "concurrently -c \"cyan,magenta\" --names \"web,cy\" \"npm run serve\" \"npm run e2e:debug\""
  },
  "dependencies": {
    "axios": "~0.16.2",
    "reflect-metadata": "^0.1.10",
    "vue": "~2.4.2",
    "vue-class-component": "~5.0.2",
    "vue-di-attributes": "git+https://github.com/Devlus/vue-js-attributes.git",
    "vue-property-decorator": "^5.2.1",
    "vue-router": "~2.7.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.6.0",
    "@types/node": "~8.0.24",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "~3.2.3",
    "bootstrap-sass": "^3.3.7",
    "compression-webpack-plugin": "~1.0.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "~4.0.1",
    "cross-env": "~5.0.5",
    "css-loader": "^0.28.5",
    "cypress": "^1.0.2",
    "es6-promise": "~4.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "~0.10.0",
    "istanbul-instrumenter-loader": "~3.0.0",
    "jasmine-core": "^2.8.0",
    "json-loader": "~0.5.7",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "~1.2.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.3",
    "karma-source-map-support": "~1.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~2.0.4",
    "lodash.merge": "~4.6.0",
    "minimist": "~1.2.0",
    "mocha": "~3.5.0",
    "ncp": "~2.0.0",
    "node-sass": "~4.5.3",
    "opn-cli": "~3.1.0",
    "postcss-loader": "^2.0.6",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "raw-loader": "~0.5.1",
    "remap-istanbul": "~0.9.5",
    "rimraf": "~2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "tslint": "~5.6.0",
    "tslint-loader": "~3.5.3",
    "typescript": "~2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "~3.5.5",
    "webpack-dev-server": "~2.7.1"
  }
}
